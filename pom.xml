						<goals>
							<goal>exec</goal>
						</goals>
						
						<configuration>
							<executable>mvn</executable>
							<arguments>
								<argument>flyway:migrate</argument>
							</arguments>
						</configuration>
					</execution>
					<execution>
						<id>jooq-db-generate</id>
