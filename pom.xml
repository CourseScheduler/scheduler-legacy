	<inceptionYear>2009</inceptionYear>
	<properties>
		<!-- Source Code and Target JVM version -->
		<maven.compiler.source>1.8</maven.compiler.source>
		<maven.compiler.target>1.8</maven.compiler.target>
		<Scheduler.j2seVersion>1.8+</Scheduler.j2seVersion>
		<maven.compiler.compilerArgument>-Xlint:all</maven.compiler.compilerArgument>

		<!-- Source file encoding -->
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

		<!-- Runnable JAR and JNLP Main Class -->
		<Scheduler.mainClass>Scheduler.Main</Scheduler.mainClass>
		<!-- JNLP root codebase -->
		<Scheduler.jnlp.server.base>http://coursescheduler.sourceforge.net</Scheduler.jnlp.server.base>

		<!-- private resource location -->
		<Scheduler.resources.base>/private/coursescheduler</Scheduler.resources.base>
		
		<!-- Jooq modules version -->
		<jooq.version>3.5.1</jooq.version>
		
		<!-- Derby dependency version -->
		<derby.version>10.11.1.1</derby.version>
		
		<!-- Database settings -->
		<db.driver>org.apache.derby.jdbc.EmbeddedDriver</db.driver>
		<derby.jdbc>jdbc:derby:</derby.jdbc>
		<db.url>${derby.jdbc}${project.build.outputDirectory}/Data/scheduler.derby</db.url>
		<db.url.setup>create=true;upgrade=true</db.url.setup>
		<db.user></db.user>
		<db.password></db.password>
		<db.generator>org.jooq.util.derby.DerbyDatabase</db.generator>
		<db.schema>APP</db.schema>
		<db.versionTable>schema_history</db.versionTable>
		<db.migration.path.0>io.devyse.scheduler.sql.derby</db.migration.path.0>
		<db.migration.path>${db.migration.path.0}</db.migration.path>
	</properties>
