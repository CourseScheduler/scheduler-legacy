	<profiles>
		<profile>
			<id>jenkins-build</id>
			<activation>
				<property>
					<name>env.BUILD_TAG</name>
				</property>
			</activation>
			<properties>
				<!-- build information -->
				<Scheduler.jnlp.build>${BUILD_TAG}</Scheduler.jnlp.build>
				<Scheduler.jnlp.qualifier>-${BUILD_NUMBER}</Scheduler.jnlp.qualifier>
			</properties>
		</profile>
		<profile>
			<id>local-sign</id>
			<activation>
				<file>
					<exists>${Scheduler.resources.base.local}</exists>
				</file>
			</activation>
			<properties>
				<Scheduler.resources.base>${Scheduler.resources.base.local}</Scheduler.resources.base>
				<jarsigner.skip>false</jarsigner.skip>
			</properties>
		</profile>
		<profile>
			<id>jenkins-sign</id>
			<activation>
				<file>
					<exists>${Scheduler.resources.base.jenkins}</exists>
				</file>
			</activation>
			<properties>
				<Scheduler.resources.base>${Scheduler.resources.base.jenkins}</Scheduler.resources.base>
				<jarsigner.skip>false</jarsigner.skip>
			</properties>
		</profile>
	</profiles>

