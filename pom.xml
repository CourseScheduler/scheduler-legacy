			<plugin>
				<groupId>org.flywaydb</groupId>
				<artifactId>flyway-maven-plugin</artifactId>
					<configuration>
						<driver>${db.driver}</driver>
						<url>${db.url};${db.url.setup}</url>
						<schemas>
							<schema>${db.schema}</schema>
						</schemas>
						<table>${db.versionTable}</table>
						<locations>
							<location>classpath:${db.migration.path.0}</location>
						</locations>
						<placeholders>
						</placeholders>
					</configuration>
			</plugin>
			<plugin>
				<groupId>org.jooq</groupId>
				<artifactId>jooq-codegen-maven</artifactId>
				<configuration>
					<jdbc>
						<driver>${db.driver}</driver>
						<url>${db.url}</url>
						<user>${db.user}</user>
						<password>${db.password}</password>
					</jdbc>
					<generator>
						<name>org.jooq.util.DefaultGenerator</name>
						<database>
							<name>${db.generator}</name>
							<includes>.*</includes>
							<excludes></excludes>
							<inputSchema>${db.schema}</inputSchema>
						</database>
						<target>
							<packageName>io.devyse.scheduler.model.jooq</packageName>
							<directory>src/generated/java</directory>
						</target>
					</generator>
				</configuration>
			</plugin>
